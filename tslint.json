{
  "extends": [
    "tslint:latest",
    "tslint-config-prettier",
    "tslint-config-airbnb",
    "tslint-react"
  ],
  "rules": {
    "import-blacklist": true,
    "import-name": [
      true,
      {
        "indexModuleCss": "styles",
        "react": "React",
        "chance": "Chance"
      }
    ],
    "member-access": false,
    "no-consecutive-blank-lines": [true, 2],
    "no-submodule-imports": [true, "antd"],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case",
      "allow-leading-underscore"
    ],
    "jsx-no-multiline-js": false,
    "object-shorthand-properties-first": false
  }
}
